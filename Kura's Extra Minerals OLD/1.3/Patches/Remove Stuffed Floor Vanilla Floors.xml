<!-- This one can only run if Royalty is detected, because it requires Royalty's Fine Tile texture.-->
<Patch>
	<Operation Class="PatchOperationFindMod">
	  <mods>
	  <li>Stuffed Floors</li>
	  </mods>
    <match Class="PatchOperationSequence">
        <success>Always</success>
	<operations>
	<li Class="PatchOperationFindMod">
		<mods>
			<li>Royalty</li>
		</mods>
    <match Class="PatchOperationInsert">
      <xpath>*/StuffedFloors.FloorTypeDef[defName = "StuffedSmoothTiles"]</xpath>
      <value>
        <StuffedFloors.FloorTypeDef ParentName="StuffedFloorsStoneRoyalBase">
          <defName>StuffedFineTiles</defName>
          <label>fine tiles</label>		
        <description>A royal floor made of exquisite stone tiles, made with no compromises, for expressing economic status. Slow to construct.</description>
        <texturePath>Terrain/Surfaces/TileStoneFine</texturePath>        
          <obsoletes>
            <!-- vanilla -->
            <li>FineTileAndesite</li>
            <li>FineTileAnorthosite</li>
            <li>FineTileBasalt</li>
            <li>FineTileBlueschist</li>
            <li>FineTileChalk</li>
			<li>FineTileCharnockite</li>
            <li>FineTileCreoleMarble</li>
            <li>FineTileDacite</li>
            <li>FineTileDunite</li>
            <li>FineTileEmperadordark</li>
			<li>FineTileEtowahMarble</li>
            <li>FineTileGabbro</li>
            <li>FineTileGreenSchist</li>
            <li>FineTileJaspillite</li>
            <li>FineTileLepidolite</li>
			<li>FineTileLherzolite</li>
            <li>FineTileLignite</li>
            <li>FineTileMigmatite</li>
            <li>FineTileMonzonite</li>
            <li>FineTileObsidian</li>
			<li>FineTileRhyolite</li>
            <li>FineTileScoria</li>
            <li>FineTileSerpentinite</li>
            <li>FineTileSiltstone</li>
            <li>FineTileSovite</li>
			<li>FineTileThometzekite</li>
			<li>FineTileSandstone</li>
            <li>FineTileGranite</li>
            <li>FineTileLimestone</li>
            <li>FineTileSlate</li>
            <li>FineTileMarble</li>
          </obsoletes>
        </StuffedFloors.FloorTypeDef>
      </value>
    </match>
	</li>
	</operations>
	</match>
	</Operation>
	
	
	
	
	
<Operation Class="PatchOperationFindMod">
	  <mods>
	  <li>Stuffed Floors</li>
	  </mods>
    <match Class="PatchOperationInsert">
      <xpath>*/StuffedFloors.FloorTypeDef[defName = "StuffedSmoothTiles"]</xpath>
      <value>
        <StuffedFloors.FloorTypeDef ParentName="StuffedFloorsStoneBasicBase">
          <defName>StuffedFlagstones</defName>
          <label>flagstones</label>		
        <description>A simple floor made of roughly-cut stone tiles. Not super pretty, but they make good surfaces for roads and outdoor walkways.</description>
        <texturePath>Terrain/Surfaces/Flagstone</texturePath>        
          <obsoletes>
            <li>FlagstoneAndesite</li>
            <li>FlagstoneAnorthosite</li>
            <li>FlagstoneBasalt</li>
            <li>FlagstoneBlueschist</li>
            <li>FlagstoneChalk</li>
			<li>FlagstoneCharnockite</li>
            <li>FlagstoneCreoleMarble</li>
            <li>FlagstoneDacite</li>
            <li>FlagstoneDunite</li>
            <li>FlagstoneEmperadordark</li>
			<li>FlagstoneEtowahMarble</li>
            <li>FlagstoneGabbro</li>
            <li>FlagstoneGreenSchist</li>
            <li>FlagstoneJaspillite</li>
            <li>FlagstoneLepidolite</li>
			<li>FlagstoneLherzolite</li>
            <li>FlagstoneLignite</li>
            <li>FlagstoneMigmatite</li>
            <li>FlagstoneMonzonite</li>
            <li>FlagstoneObsidian</li>
			<li>FlagstoneRhyolite</li>
            <li>FlagstoneScoria</li>
            <li>FlagstoneSerpentinite</li>
            <li>FlagstoneSiltstone</li>
            <li>FlagstoneSovite</li>
			<li>FlagstoneThometzekite</li>
			<li>FlagstoneSandstone</li>
            <li>FlagstoneGranite</li>
            <li>FlagstoneLimestone</li>
            <li>FlagstoneSlate</li>
            <li>FlagstoneMarble</li>
          </obsoletes>
        </StuffedFloors.FloorTypeDef>
      </value>
    </match>
	</Operation>
	
	
	
	
	
